{
    "name": "hiqdev/yii2-hiart-httpclient",
    "type": "yii2-extension",
    "description": "yii2-httpclient transport for yii2-hiart",
    "keywords": [
        "yii2",
        "hiart",
        "httpclient"
    ],
    "homepage": "https://github.com/hiqdev/yii2-hiart-httpclient",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/yii2-hiart-httpclient",
        "issues": "https://github.com/hiqdev/yii2-hiart-httpclient/issues",
        "wiki": "https://github.com/hiqdev/yii2-hiart-httpclient/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "yiisoft/yii2-httpclient": "~2.0.0"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\hiart\\httpclient\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "src/config/params.php",
            "hisite": "src/config/common.php",
            "hidev": "src/config/common.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
